<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              xmlns:tools="http://schemas.android.com/tools"
              android:id="@+id/layout_bus_a"
              android:layout_width="match_parent"
              android:layout_height="match_parent">

    <fragment
            android:id="@+id/map"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:name="com.google.android.gms.maps.SupportMapFragment" />

    <ImageButton
            android:layout_width="143dp"
            android:layout_height="60dp"
            android:id="@+id/switchToA"
            android:padding="0dp"
            android:src="@drawable/a_button"
            android:background="#ffffff"
            android:layout_gravity="left|bottom"/>

    <ImageButton
            android:layout_width="130dp"
            android:layout_height="60dp"
            android:src="@drawable/move_to_cnu"
            android:layout_gravity="center|bottom"
            android:background="#ffffff"
            android:id="@+id/moveToCNU"/>

    <ImageButton
            android:layout_width="143dp"
            android:layout_height="60dp"
            android:id="@+id/switchToB"
            android:padding="0dp"
            android:src="@drawable/b_button"
            android:background="#ffffff"
            android:layout_gravity="right|bottom"/>

    <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:textSize="24dp"
            android:textColor="#000000"
            android:layout_gravity="top"
            android:text="잠시만 기다려주세요.."
            android:id="@+id/textView"/>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" android:id="@+id/textView1" android:layout_weight="1"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" android:id="@+id/textView2" android:layout_weight="1"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" android:id="@+id/textView3" android:layout_weight="1"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" android:id="@+id/textView4" android:layout_weight="1"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" android:id="@+id/textView5" android:layout_weight="1"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" android:id="@+id/textView6" android:layout_weight="1"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" android:id="@+id/textView7" android:layout_weight="1"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" android:id="@+id/textView8" android:layout_weight="1"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" android:id="@+id/textView9" android:layout_weight="1"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" android:id="@+id/textView10" android:layout_weight="1"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" android:id="@+id/textView11" android:layout_weight="1"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" android:id="@+id/textView12" android:layout_weight="1"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" android:id="@+id/textView13" android:layout_weight="1"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" android:id="@+id/textView14" android:layout_weight="1"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" android:id="@+id/textView15" android:layout_weight="1"/>
    </LinearLayout>


</FrameLayout>